{
    "task": "1",
    "img_size" : 128,
    "img_channels" : 3,
    "cpu_threads": 16,
    "is_training": "train",
    "gpu_id": "6",

    "datasets": {
       "train":{
            "dir":"./Dataprep/results/train1.tfrecord"
       },
       "val":{
            "dir":"./Dataprep/results/val1.tfrecord"
       },
       "test":{
           "dir":"./Dataprep/results/test1.txt"
       }
    },

    "networks": {
        "which_model": "MODEL_RESNET",
        "drop_rate": 0.5
    },

    "solver": {
